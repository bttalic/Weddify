<%= form_for  @item do |f| %>
<%= render 'shared/error_messages', object: @item %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :link %>
<%= f.text_field :link %>
<%= f.label :priority %>
<%= f.number_field :priority, :in => 1...11%>
<%= f.submit 'Add Item' %>
<% end %>
<%= render 'users/wishlist'  %>