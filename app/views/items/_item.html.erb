<% @items.each do |item| %>
<%= item.name %> <%= link_to(:link, url_with_protocol(item.link), :target => "_blank") %> <%=  item.priority %> <%= item.status %> 
<% if current_user?(User.find(item.user_id)) %>
<%= link_to t('delete'), item, method: :delete,
confirm: t('confirm'),
title: item.name %>
<% end %>
</br>
<% end %>