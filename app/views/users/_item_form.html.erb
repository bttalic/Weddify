<%= form_for(@user.wishlist.item) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :link %>
<%= f.text_field :link %>
<%= f.label :priority %>
<%= f.number_field :priority, :in => 1...11%>
<%= f.submit t('add') %>
<% end %>

